<!-- Fixpositioning of robots in manual-mode -->

<MASKS>
	<MASKDEF maskkey="TeachView.mask.SCU_Fixpos" textdef="userTx" permissiondef="userMenuPermission">
		<GROUP key="SCU_Fixpos.group" columns="40" rows="9" col="1" row="1" halign="stretch" valign="stretch">
		
			<LABEL    key="SCU_Fixpos.label.Robot"    cspan="3"                                                            col="1"   row="2"    halign="center"/>
			<NUMFIELD var="SCU.SV_SCU2HMI.tFixpos.i_iRobotNo"   vartype="SV"                                               col="10"   row="2" enabled="true"  halign="center" />
			<LABEL    var="SCU.SV_ScuControlPanel.zFixposRobot" vartype="SV"                         cspan="4"             col="22"   row="2"    enabled="FALSE"                 halign="left"/>
			
			<LABEL    key="SCU_Fixpos.label.FixposNo" cspan="3"                                                            col="1"   row="3"    halign="center"/>
			<NUMFIELD var="SCU.SV_SCU2HMI.tFixpos.i_iFixposNo"  vartype="SV"                                               col="10"   row="3"  enabled="true"  halign="center" />
			
			<LABEL     key="SCU_Fixpos.label.ToAll"    cspan="3"                                                           col="1"   row="5"    halign="center"/>
			<CHECKBOX  var="SCU.SV_SCU2HMI.tFixpos.i_mOptToAll"  cspan="3"                   vartype="SV"                  col="10"  row="5"    enabled="true" />
			
			
			<BUTTON key="SCU_Fixpos.but.Start"      cspan="8"  var="SCU.SV_SCU2HMI.tFixpos.i_mStartFixpos"    vartype="SV" col="1"   row="7"  enablevar="SCU.SV_SCU2HMI.tFixpos.o_mRelStart" enablevartype="SV"/>
			<BUTTON key="SCU_Fixpos.but.Reset"      cspan="8"  var="SCU.SV_SCU2HMI.tFixpos.i_mReset"          vartype="SV" col="10"  row="7"  />
			
			
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[1].iRobotID"     vartype="SV"                   cspan="2"            col="31"  row="2"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[1].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[1].zRobotName"   vartype="SV"                   cspan="4"            col="33"  row="2"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[1].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[1].iActFixposNo" vartype="SV"                   cspan="2"            col="37"  row="2"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[1].mEnabled"    visiblevartype="SV"/>
			
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[2].iRobotID"     vartype="SV"                   cspan="2"            col="31"  row="3"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[2].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[2].zRobotName"   vartype="SV"                   cspan="4"            col="33"  row="3"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[2].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[2].iActFixposNo" vartype="SV"                   cspan="2"            col="37"  row="3"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[2].mEnabled"    visiblevartype="SV"/>
			
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[3].iRobotID"     vartype="SV"                   cspan="2"            col="31"  row="4"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[3].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[3].zRobotName"   vartype="SV"                   cspan="4"            col="33"  row="4"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[3].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[3].iActFixposNo" vartype="SV"                   cspan="2"            col="37"  row="4"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[3].mEnabled"    visiblevartype="SV"/>
			
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[4].iRobotID"     vartype="SV"                   cspan="2"            col="31"  row="5"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[4].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[4].zRobotName"   vartype="SV"                   cspan="4"            col="33"  row="5"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[4].mEnabled"    visiblevartype="SV"/>
			<LABEL    var="SCU.SV_ScuControlPanel.yFG[4].iActFixposNo" vartype="SV"                   cspan="2"            col="37"  row="5"    enabled="FALSE"                  halign="left"    visiblevar="SCU.SV_ScuControlPanel.yFG[4].mEnabled"    visiblevartype="SV"/>

		</GROUP>
	</MASKDEF>
</MASKS>