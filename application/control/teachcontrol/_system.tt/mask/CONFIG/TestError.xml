<VIEW>
   <MASKDEF key="TeachView.mask.TestError" textfile="userTx" permissiondef="userMenuPermission">
      <GROUP key="TestError.group"  rows="25" columns="30" row="1" col="1" rspan="30" cspan="30" halign="stretch" valign="stretch">   
       	  	
	  <LABEL key="EnableTestError" col="1" row="2" cspan="4" halign="stretch"/>
	  <CHECKBOX var="system.SV_TestError.mErrorRoutineActive" vartype="SV" enabled="TRUE" col="7" row="2" cspan="3"/>

	  <LABEL key="SetError" col="16" row="2" cspan="4" halign="stretch"/>
	  <CHECKBOX var="system.SV_TestError.mSetError" vartype="SV" enablevar="system.SV_TestError.mErrorRoutineActive" enablevartype="SV" col="20" row="2" cspan="3"/>

	  <LABEL key="Component:" col="1" row="5" cspan="4" halign="left"/>
	  <NUMFIELD var="system.SV_TestError.tBuildNewID.iComponentNumber" vartype="SV" enabled="TRUE" col="6" row="5"  halign="stretch" cspan="2"/>  
	  <LABEL key="Message:" col="1" row="7" cspan="4" halign="left"/>
	  <NUMFIELD var="system.SV_TestError.tBuildNewID.iMessageNumber" vartype="SV" enabled="TRUE" col="6" row="7"  halign="stretch" cspan="2"/>  
	  <LABEL key="MsgCount:" col="1" row="9" cspan="4" halign="left"/>
	  <NUMFIELD var="system.SV_TestError.tErrorData.i_iMsgCount" vartype="SV" enabled="TRUE" col="6" row="9"  halign="stretch" cspan="2"/>  
	  <LABEL key="DeviceNo:" col="1" row="11" cspan="4" halign="left"/>
	  <NUMFIELD var="system.SV_TestError.tErrorData.i_iDeviceNr" vartype="SV" enabled="TRUE" col="6" row="11"  halign="stretch" cspan="2"/>  
  	  <LABEL key="FgNumber:" col="1" row="16" cspan="4" halign="left"/>
          <NUMFIELD var="system.SV_TestError.tErrorData.i_iFgNumber" vartype="SV" enabled="TRUE" col="8" row="16" rspan="1" cspan="1" halign="stretch"/>
 	  <LABEL key="FgName" var="system.SV_TestError.tErrorData.o_zFgName" vartype="SV" enabled="TRUE" col="10" row="16" rspan="1" cspan="3" halign="stretch"/>
 	  <LABEL key="FuName:" col="1" row="19" cspan="4" halign="left"/>
          <TEXTFIELD var="system.SV_TestError.tErrorData.i_zFuName" vartype="SV" enabled="TRUE" col="6" row="19" rspan="1" cspan="3" halign="stretch"/>
	  <LABEL key="Class:" col="1" row="22" cspan="4" halign="left"/>
