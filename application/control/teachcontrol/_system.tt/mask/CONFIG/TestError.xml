	  <LABEL key="MsgClass" var="system.SV_TestError.tErrorData.o_zErrorClass" vartype="SV" enabled="TRUE"  col="10" row="22" cspan="4" halign="left"/>

	  <LABEL key="Location:" col="10" row="5" cspan="4" halign="left"/>
          <TEXTFIELD var="system.SV_TestError.tErrorData.i_zLocation" vartype="SV" enabled="TRUE" col="15" row="5" rspan="1" cspan="6" halign="stretch"/>
	  <LABEL key="TPName:" col="10" row="7" cspan="4" halign="left"/>
          <TEXTFIELD var="system.SV_TestError.tErrorData.i_zTimeProgName" vartype="SV" enabled="TRUE" col="15" row="7" rspan="1" cspan="6" halign="stretch"/>
	  <LABEL key="Param1:" col="10" row="9" cspan="4" halign="left"/>
          <TEXTFIELD var="system.SV_TestError.tErrorData.i_zParam1" vartype="SV" enabled="TRUE" col="15" row="9" rspan="1" cspan="6" halign="stretch"/>
	  <LABEL key="Param2:" col="10" row="11" cspan="4" halign="left"/>
          <TEXTFIELD var="system.SV_TestError.tErrorData.i_zParam2" vartype="SV" enabled="TRUE" col="15" row="11" rspan="1" cspan="6" halign="stretch"/>
	  <LABEL key="Param3:" col="10" row="13" cspan="4" halign="left"/>
          <TEXTFIELD var="system.SV_TestError.tErrorData.i_zParam3" vartype="SV" enabled="TRUE" col="15" row="13" rspan="1" cspan="6" halign="stretch"/>
                   
	  <LABEL key="TP_Step:" col="24" row="5" cspan="4" halign="left"/>
	  <NUMFIELD var="system.SV_TestError.tErrorData.i_iTimeProgStep" vartype="SV" enabled="TRUE" col="28" row="5"  halign="stretch" cspan="2"/>  
	  <LABEL key="Channel:" col="24" row="7" cspan="4" halign="left"/>
	  <NUMFIELD var="system.SV_TestError.tErrorData.i_iChannel" vartype="SV" enabled="TRUE" col="28" row="7"  halign="stretch" cspan="2"/>  
	    
      </GROUP>
	    
      <MENUDEF key="TeachView.mask.TestError" textdef="userTx"  imagefile="KvtMenuIcon" permissiondef="userMenuPermission">  
      </MENUDEF>		  
   </MASKDEF>
</VIEW>

