<VIEW>

   <MASKDEF key="TeachView.mask.driveParam"  
            textfile="userTx"  permissiondef="userMenuPermission" >  
                      
         <GROUP key="DriveParam.label.seldrive" columns="40" rows="4" col="1" row="1" rspan="4" halign="stretch" valign="stretch">
             
             <LABEL key="DriveParam.label.drivenr" col="5" row="2" cspan="1" halign="left"/>
             <NUMFIELD var="_system.RcuUserPage.iUsedDrive" vartype="TT" enabled="true" halign="stretch" col="13" row="2" cspan="2"/>
             
             <LABEL key="DriveParam.label.datachg" col="25" row="2" cspan="1" halign="left"/>
             <CHECKBOX var="_system.RcuUserPage.Drive.bDataChanged" vartype="TT" col="35" row="2" enabled="false" halign="left" cspan="1"/>
             
         </GROUP>    
             
         <GROUP key="DriveParam.label.posdrive" columns="40" rows="8" col="1" row="5" rspan="6" halign="stretch" valign="stretch">
                      
             <LABEL key="DriveParam.label.minpos" col="5" row="2" cspan="1" halign="left"/>
             <LABEL key="DriveParam.label.s50" col="5" row="3" cspan="1" halign="left"/>
             <NUMFIELD var="_system.RcuUserPage.Drive.rMinPosDrive" vartype="TT" enabled="true" halign="stretch" col="5" row="5" cspan="6"/> 
             
             <LABEL key="DriveParam.label.maxpos" col="30" row="2" cspan="1" halign="left"/>
             <LABEL key="DriveParam.label.s49" col="30" row="3" cspan="1" halign="left"/>
             <NUMFIELD var="_system.RcuUserPage.Drive.rMaxPosDrive" vartype="TT" enabled="true" halign="stretch" col="30" row="5" cspan="6"/> 
             
             <LABEL key="DriveParam.label.zeropos" col="18" row="2" cspan="1" halign="left"/>
             <LABEL key="DriveParam.label.s52" col="18" row="3" cspan="1" halign="left"/>
             <NUMFIELD var="_system.RcuUserPage.Drive.rZeroPosDrive" vartype="TT" enabled="true" halign="stretch" col="18" row="5" cspan="6"/> 
             
         </GROUP>  
         
         <GROUP key="DriveParam.label.poscfg" columns="40" rows="8" col="1" row="11" rspan="6" halign="stretch" valign="stretch">
         
             <LABEL key="DriveParam.label.minpos" col="8" row="2" cspan="1" halign="left"/>
             <LABEL key="DriveParam.label.cfgfile" col="8" row="3" cspan="1" halign="left"/>
             <NUMFIELD var="_system.RcuUserPage.Drive.rMinPosCfg" vartype="TT" enabled="true" halign="stretch" col="8" row="5" cspan="6"/> 
             
             <LABEL key="DriveParam.label.maxpos" col="28" row="2" cspan="1" halign="left"/>
             <LABEL key="DriveParam.label.cfgfile" col="28" row="3" cspan="1" halign="left"/>
             <NUMFIELD var="_system.RcuUserPage.Drive.rMaxPosCfg" vartype="TT" enabled="true" halign="stretch" col="28" row="5" cspan="6"/> 
             
         </GROUP>	  
         
         <GROUP key="DriveParam.label.info" columns="40" rows="3" col="1" row="17" rspan="3" halign="stretch" valign="stretch">
            <LABEL key="DriveParam.label.messages" var="_system.RcuUserPage.Drive.sUserInfo" vartype="TT" enabled="false" col="5" row="1" cspan="30" rspan="1" halign="stretch"/>
         </GROUP>
         
	 
   </MASKDEF>
   
   <MENUDEF key="TeachView.mask.driveParam" textdef="userTx"  imagefile="KvtMenuIcon">
	<MENU>
		<MENUITEM key="DriveParam.menu.button_0" enabled="true" var="_system.RcuUserPage.Drive.bButtonRefresh" vartype="TT"/>
		<MENUITEM key="DriveParam.menu.button_1" enablevar="_system.gRcuFocusedRobot.autoControl.bBitT1" enablevartype="TT" var="_system.RcuUserPage.Drive.bButtonStore" vartype="TT"/>
		<MENUITEM type="empty"/>
		<MENUITEM type="empty"/>
		<MENUITEM key="DriveParam.menu.button_4" enabled="true" var="_system.RcuUserPage.Drive.bButtonDown" vartype="TT"/>
		<MENUITEM key="DriveParam.menu.button_5" enabled="true" var="_system.RcuUserPage.Drive.bButtonUp" vartype="TT"/>
	</MENU>
   </MENUDEF>
   
   
	
</VIEW>