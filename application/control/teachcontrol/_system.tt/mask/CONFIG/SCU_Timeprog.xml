
<MASKS>

   <MASKDEF maskkey="TeachView.mask.SCU_Timeprog" textdef="userTx" permissiondef="userMenuPermission">
       <GROUP key="Timeprogram" columns="40" rows="16" col="1" row="1" halign="stretch" valign="stretch">

         <LABEL key="Timeprogram name" col="2" row="3" cspan="5" halign="center"/>
         <TEXTFIELD var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_zTimeProgName" vartype="SV" enabled="TRUE" col="2" row="4" cspan="16" halign="stretch"/>
         <LABEL key="Channel" col="20" row="3" cspan="5" halign="center"/>
         <TEXTFIELD var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_zTimeProgChannel" vartype="SV" enabled="TRUE" col="20" row="4" cspan="5" halign="stretch"/>
         <LABEL key="State" col="30" row="3" cspan="4" halign="center"/>
         <NUMFIELD var="PCU1.SV_PCU2HMI.tTimeProg.sOut.o_iTimeProgState" vartype="SV" enabled="FALSE" col="30" row="4" cspan="4" halign="stretch"/>
         <LABEL key="Not reset at start" col="2" row="6" cspan="6" halign="center"/>
         <CHECKBOX var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_mNotResetSignalsAtStart" vartype="SV" col="12" row="6" enabled="TRUE" cspan="2"/>  
         <LABEL key="Not reset at stop" col="2" row="8" cspan="6" halign="center"/>
         <CHECKBOX var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_mNotResetSignalsAtStop" vartype="SV" col="12" row="8" enabled="TRUE" cspan="2"/>  
         <BUTTON key="Start" cspan="6" var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_mTimeProgStart" vartype="SV" enablevar="PCU1.SV_PCU2HMI.tTimeProg.sOut.o_mTimeProgStartEn" enablevartype="SV" col="2" row="10" /> 
         <BUTTON key="Reset" cspan="6" var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_mTimeProgStop" vartype="SV" enablevar="PCU1.SV_PCU2HMI.tTimeProg.sOut.o_mTimeProgStopEn" enablevartype="SV" col="10" row="10" /> 
         <BUTTON key="ResetAll" cspan="6" var="PCU1.SV_PCU2HMI.tTimeProg.sIn.i_mTimeProgStopAll" vartype="SV" enablevar="PCU1.SV_PCU2HMI.tTimeProg.sOut.o_mTimeProgStopAllEn" enablevartype="SV" col="18" row="10" /> 
         
      </GROUP>

   </MASKDEF> 
		
<MENUDEF maskkey="TeachView.mask.Timeprog" textdef="userTx" permissiondef="userMenuPermission" imagedef="KvtMenuIcon">
	<MENU>
	  <MENUITEM key="TeachView.mask.AutoControl" maskkey="TeachView.mask.AutoControl" enabled="true"/>
	  <MENUITEM key="Fixpos" maskkey="TeachView.mask.Fixpos" enabled="true"/>
	  <MENUITEM type="empty"/>
	  <MENUITEM type="empty"/>
	  <MENUITEM type="empty"/>
	  <MENUITEM type="empty"/>
	</MENU>
</MENUDEF>

</MASKS>

