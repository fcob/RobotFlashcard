<VIEW>
   <MASKDEF key="TeachView.mask.brakeCheck" textfile="userTx"  permissiondef="userMenuPermission" controlvar="_system.rcuuserpage.bBrakeCheckPageOpen" controlvartype="TT">
       <GROUP key="BrakeCheck.label.brkchk" rows="10" rspan="10" columns="40" row="1" col="1" halign="stretch" valign="stretch">
           <GROUP key="BrakeCheck.label.stateaxes" rows="10" columns="40" row="1" col="1" rspan="10" cspan="40" halign="stretch" valign="stretch">
           <LABEL key="BrakeCheck.label.stateaxes_ex" col="1" row="1" cspan="30" halign="left"/>      
               
               <TABLE key="brakeCheck.axisTable" col="1" row="2" cspan="24" rows="8" rspan="8" halign="stretch" valign="stretch" enabled="false">
                    <COLUMN>
									  	<ENTRY key=" " width="4"/>
									  	<ENTRY key=" " width="22"/>
									  </COLUMN>
									  <!-- Joint 1 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[0]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[0]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate1" var="_system.gRcuFocusedRobot.drive[0].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 2 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[1]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[1]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate2" var="_system.gRcuFocusedRobot.drive[1].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 3 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[2]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[2]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate3" var="_system.gRcuFocusedRobot.drive[2].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 4 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[3]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[3]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate4" var="_system.gRcuFocusedRobot.drive[3].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 5 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[4]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[4]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate5" var="_system.gRcuFocusedRobot.drive[4].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 6 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[5]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[5]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate6" var="_system.gRcuFocusedRobot.drive[5].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 7 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[6]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[6]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate7" var="_system.gRcuFocusedRobot.drive[6].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>
							                  <!-- Joint 8 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[7]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[7]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate8" var="_system.gRcuFocusedRobot.drive[7].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>

									  						<!-- Joint 9 -->
							                  <ROW visiblevar="_system.gRcSelectedRobotData.showDrive[8]" visiblevartype="TT" halign="stretch" valign="stretch">
									      <ENTRY key="drive1_name"                 var="_system.gRcSelectedRobotData.jointName[8]"	              vartype="TT"/>		
									      <LABEL key="brakeCheck.label.axisstate8" var="_system.gRcuFocusedRobot.drive[8].brakeCheck.iExecState"  vartype="TT"/>
							
									  </ROW>

               </TABLE>
               
               
           <!-- Robot in brakecheck position -->
           <GROUP key="BrakeCheck.label.staterob" visiblevar="_system.gRcuFocusedRobot.brakeCheck.bNoTimer" visiblevartype="TT"
                  rows="10" columns="14" row="1" col="25" rspan="10" cspan="15" halign="stretch" valign="stretch">   
               <LABEL key="BrakeCheck.label.robin" col="2" row="3" cspan="13" halign="left"/>
               <LABEL key="BrakeCheck.label.testpos1" col="2" row="4" cspan="13" halign="left"/>
               <CHECKBOX var="_system.gRcuFocusedRobot.fixpos.RcuInBrakeCheckPos" vartype="TT" 
                         visiblevar="_system.gRcuFocusedRobot.brakeCheck.bNoTimer" visiblevartype="TT"
                         col="6" row="5" enabled="false" halign="left" cspan="1"/>    
           </GROUP>
           <!-- Time until next check necessary -->
           <GROUP key="BrakeCheck.label.time" visiblevar="_system.gRcuFocusedRobot.brakeCheck.bUseTimer" visiblevartype="TT"
                  rows="10" columns="15" row="1" col="25" rspan="10" cspan="15" halign="stretch" valign="stretch">
                  
              <LABEL key="BrakeCheck.label.time" col="1" row="1" cspan="15" halign="left"/>                  
               <TEXTFIELD var="_system.gRcuFocusedRobot.brakeCheck.sTimeNextCheck" vartype="TT" enabled="false" 
                          col="4" row="2" cspan="7" rspan="1" halign="stretch"/> 
                                         
           </GROUP>
           <!-- Robot in brakecheck position -->
           <GROUP key="BrakeCheck.label.testpos2" visiblevar="_system.gRcuFocusedRobot.brakeCheck.bUseTimer" visiblevartype="TT"
                  rows="10" columns="15" row="1" col="25" rspan="10" cspan="15" halign="stretch" valign="stretch">   
               
               <LABEL key="BrakeCheck.label.testpos2" col="1" row="3" cspan="15" halign="left"/> 
               <CHECKBOX var="_system.gRcuFocusedRobot.fixpos.RcuInBrakeCheckPos" vartype="TT"
                         col="4" row="4" enabled="false" halign="stretch" valign="stretch" cspan="3" rspan="1"/>      
           </GROUP>
           
           
           
            <!-- state and button for brakecheck -->
           <GROUP key="BrakeCheck.label.statetst" visiblevar="_system.gRcuFocusedRobot.brakeCheck.bNoTimer" visiblevartype="TT"
                  rows="4" columns="15" row="6" col="25" rspan="4" cspan="15" halign="stretch" valign="stretch">  
               
               <LABEL key="BrakeCheck.label.statetst" col="2" row="1" cspan="14" halign="left"/> 
               <BUTTON key="BrakeCheck.button.start" var="_system.gRcuFocusedRobot.brakeCheck.UserPageData.bStartFromTV" vartype="TT" 
                       col="2" row="2" cspan="10" rspan="1" halign="stretch"/>               
           </GROUP>
           
           <!-- movement relese -->
           <GROUP key="BrakeCheck.label.movrel" visiblevar="_system.gRcuFocusedRobot.brakeCheck.bUseTimer" visiblevartype="TT"
                  rows="11" columns="15" row="5" col="25" rspan="6" cspan="15" halign="stretch" valign="stretch">
                  
               <LABEL key="BrakeCheck.label.movrel" col="1" row="1" cspan="14" halign="left"/>   
               <TEXTFIELD var="_system.gRcuFocusedRobot.brakeCheck.sTimeMovementRel"  vartype="TT" enabled="false" col="4" row="2" cspan="07" rspan="1" halign="stretch"/> 
               <BUTTON var="_system.gRcuFocusedRobot.brakeCheck.UserPageData.bMovementRelease" vartype="TT" 
                       enablevar="_system.gRcuFocusedRobot.brakeCheck.UserPageData.bRequestMovementRelease" enablevartype="TT" 
                       key="BrakeCheck.button.rel" col="4" row="3" cspan="10" rspan="1" halign="stretch"/> 
           </GROUP>  
           
         <!--   state and button for brakecheck  -->
           <GROUP key="BrakeCheck.label.test" visiblevar="_system.gRcuFocusedRobot.brakeCheck.bUseTimer" visiblevartype="TT"
                  rows="2" columns="15" row="8" col="25" rspan="2" cspan="15" halign="stretch" valign="stretch">  
               
               <LABEL key="BrakeCheck.label.test" col="1" row="1" cspan="14" halign="left"/>
               <BUTTON key="BrakeCheck.button.start2" var="_system.gRcuFocusedRobot.brakeCheck.UserPageData.bStartFromTV" vartype="TT" 
                       col="4" row="2" cspan="10" rspan="1" halign="stretch"/>               
           </GROUP> 



           </GROUP>
       </GROUP>
   </MASKDEF>
</VIEW>