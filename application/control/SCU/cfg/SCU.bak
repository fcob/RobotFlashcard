[global/StationConfig]
Burner          = 0,*,0,R,0,1        ;1..this station has a flaming-robot (special handling for conveyor-release)
EnPurgeCounters = 0,*,1,R,0,1        ;1..enable purge-counters for postprocess-flags in PLC
FastGhStop      = 0,*,0,R,0,1        ;1..each robot finishes just his object (fast), 0..process all ghosts in zone
FifoSizeInlet   = 1,*,1,R,1,10       ;number of used inlet-fifo positions
FifoSizeZone    = 1,*,2,R,1,10       ;number of used zone-fifo positions
HighTension     = 0,*,1,R,0,1        ;1..high-tension available on this station
HtGroundDelay   = 1,*,5,R,0,10       ;time-delay of lamp "high tension is grounded" (s)
InTouch         = 0,*,0,R,0,1        ;1..use InTouch-HMI, 0..use Teachpendant-Userpages
MinBodyGapSG    = 1,*,10,R,2,100     ;STOP&GO: min. gap between ghost-objects (s)
MinBodyGapTrck  = 1,*,70,R,10,1000   ;TRACKING: min. gap between ghost-objects (cm)
PNOZmulti       = 0,*,0,R,0,1        ;1..enable communication with PNOZmulti as safety-plc
Sealing         = 0,*,0,R,0,1        ;1..Sealing, 0..Paint (e.g. T2 with material, lock main-needles in ghost,...)
SkidLength      = 1,*,530,R,0,1000   ;TRACKING: length of skid (e.g. for lockcounters) (cm)
UniNoPaint      = 0,*,0,R,0,1        ;1..do not process objects with uni-color (in AirBc-Stations)
